#include <iostream>
using namespace std;

class Queue{
    int size;
    int front;
    int rear;
    int *q;
    public:
    Queue(int size){
        this->size=size;
        front=rear=-1;
        q=new int[this->size];
    }
    void enqueue(int x);
    int dequeue();
    void display();
};

void Queue :: enqueue(int x){
    if(rear==size-1){cout<<"queue is full";}
    else{
        rear++;
        q[rear]=x;
    }
}

int Queue :: dequeue(){
    int x=-1;
    if(rear==front){cout<<"queue is empty";}
    else{
        front++;
        x=q[front];
    }
    return x;
}

void Queue :: display(){
    if(rear==front){cout<<"queue is empty";}
    for(int i=front+1;i<=rear;i++){
        cout<<q[i]<<endl;
    }
    cout<<"\n";
}

int main() {
    int size;
    cout<<"enter size of the queue: ";
    cin>>size;
    Queue Q(size);
    Q.enqueue(13);
    Q.enqueue(7);
    Q.enqueue(4);
    Q.enqueue(1);
    Q.enqueue(6);
    Q.enqueue(8);
    Q.enqueue(10);
    Q.display();
    cout<<"the deleted element is: "<<Q.dequeue()<<endl;
    cout<<"the deleted element is: "<<Q.dequeue()<<endl;
    cout<<"the deleted element is: "<<Q.dequeue()<<endl;
    Q.display();
    return 0;
}
